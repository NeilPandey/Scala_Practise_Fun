package com.class1.object1

//Method overloading

class MetOload {
  println("hello"); //default
  def add(a:Int, b:Int): Unit ={
    var sum = a+b
    println(sum)
  }

def add(a:Int,b:Int,c:Int) = {
  var sum = a+(b*c)
  println(sum)
}
}
object Mload {
  def main(args:Array[String]): Unit ={
    val M = new MetOload();
    println("hey")
    M.add(10,10)
    M.add(10,3,8)
  }
}
