class ArrayExample{  
    var arr = Array(Array(1,2,3,4,5), Array(6,7,8,9,10))   // Creating multidimensional array  
    def show(){  
        for(i<- 0 to 1){               // Traversing elements using loop  
           for(j<- 0 to 4){  
                print(" "+arr(i)(j))  
            }  
            println()  
        }      
    }  
}  
  
object MainObject{  
    def main(args:Array[String]){  
        var a = new ArrayExample()  
        a.show()                       
    }  
}  
